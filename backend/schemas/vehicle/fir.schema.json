{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Vehicle - Police Report",
  "type": "object",
  "properties": {
    "police_report_no": {"type": "string"},
    "claim_id": {"type": "string"},
    "report_date": {"type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$"},
    "incident_date": {"type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$"},
    "location": {"type": "string"},
    "registration": {"type": "string"},
    "rc_no": {"type": "string"},
    "dl_no": {"type": "string"},
    "injuries_reported": {"type": "boolean"},
    "estimated_damage_cost": {"type": "number"}
  },
  "required": [
    "police_report_no",
    "claim_id",
    "incident_date",
    "registration",
    "rc_no",
    "dl_no"
  ]
}
