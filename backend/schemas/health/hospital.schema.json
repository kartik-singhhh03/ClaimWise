{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Health - Hospital Report",
  "type": "object",
  "properties": {
    "claim_id": {"type": "string"},
    "patient_id": {"type": "string"},
    "hospital_code": {"type": "string"},
    "prescription": {"type": "string"},
    "admission_date": {"type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$"},
    "discharge_date": {"type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$"},
    "bill_amount": {"type": "number"}
  },
  "required": [
    "claim_id",
    "patient_id",
    "hospital_code",
    "bill_amount"
  ]
}
